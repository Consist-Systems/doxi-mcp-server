{
  "AllowedOrigins": null,
  "DisplayVersion": "1.0.0",
  "GPTDataExtraction": {
    "GPTAPIKey": null,
    "TemplateExtractorFromPDFConfig": {
      "Model": "gpt-4o",
      "Endpoint": "https://api.openai.com/v1/chat/completions",
      "MaxPagesPerBatch": 10
    },
    "TemplateExtractorFromTextConfig": {
      "Model": "ft:gpt-4.1-nano-2025-04-14:personal:doxicreatetemplateinformation:CcsWe2l4",
      "Endpoint": "https://api.openai.com/v1/responses"
    }
  },
  "NLog": {
    "throwExceptions": "false",
    "extensions": [
      { "assembly": "NLog.Extensions.Logging" },
      { "assembly": "NLog.Web.AspNetCore" }
    ],
    "targets": {
      "async": true,
      "consolelog": {
        "type": "ColoredConsole",
        "layout": {
          "type": "JsonLayout",
          "includeMdlc": "true",
          "attributes": [
            {
              "name": "date",
              "layout": "${longdate}"
            },
            {
              "name": "ipaddress",
              "layout": "${eventcontext:ip_address}"
            },
            {
              "name": "serveripaddress",
              "layout": "${aspnet-Request-Host}"
            },
            {
              "name": "level",
              "layout": "${level:upperCase=true}"
            },
            {
              "name": "duration",
              "layout": "${eventcontext:duration}"
            },
            {
              "name": "message",
              "layout": "${message}"
            },
            {
              "name": "exception",
              "layout": "${exception:format=ToString}"
            },
            {
              "name": "stacktrace",
              "layout": "${stacktrace}"
            }
          ]
        }
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "consolelog"
      },

      {
        "logger": "Microsoft.*",
        "maxLevel": "Warn",
        "final": "true"
      }
    ]
  },
  "DoxiAPIClient": {
    "IdpURL": "http://keycloak:8080/auth",
    "DoxiAPIUrl": "http://localhost:5000"
  }
}
